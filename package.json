{
  "name": "iccsesrver",
  "version": "0.2.3",
  "private": true,
  "scripts": {
    "front-test": "jest",
    "front-test:watch": "jest --watch",
    "front-test-coverage": "jest --coverage",
    "start": "meteor run",
    "test": "meteor test --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "prepare": "husky insta ll",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.(js|ts|tsx|json)\"",
    "check-types": "tsc",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run check-types && npm run check-format && npm run lint && npm run front-test"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/chess.js": "^0.11.2",
    "chess.js": "^0.12.1",
    "chessboard": "https://github.com/chessclub/v2icc-chessboard.git",
    "client": "0.0.1",
    "clsx": "^1.1.1",
    "eventemitter3": "^4.0.7",
    "formik": "^2.2.9",
    "ip": "^1.1.5",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "meteor-node-stubs": "^1.1.0",
    "react": "^17.0.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.2",
    "react-grid-layout": "^1.3.0",
    "react-jss": "^10.9.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "5.3.0",
    "react-visibility-sensor": "^5.1.1",
    "simpl-schema": "^1.12.0",
    "slack-notify": "^0.1.7",
    "ts-jest": "^27.1.3"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/chai": "^4.3.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.177",
    "@types/meteor": "^1.4.87",
    "@types/meteor-universe-i18n": "^1.14.6",
    "@types/mocha": "^9.0.0",
    "@types/react": "^17.0.35",
    "@types/react-custom-scrollbars": "^4.0.10",
    "@types/react-dom": "^17.0.11",
    "@types/react-grid-layout": "^1.3.0",
    "@types/react-router-dom": "^5.3.2",
    "@types/sinon": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chromedriver": "^96.0.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-meteor": "^7.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "puppeteer": "^13.0.1",
    "selenium-webdriver": "^3.0.0-beta-2",
    "sinon": "^12.0.1",
    "typescript": "^4.5.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint"
    ],
    "**/*.(js|ts|tsx|json)": [
      "prettier --write",
      "jest --findRelatedTests",
      "git add"
    ]
  }
}
